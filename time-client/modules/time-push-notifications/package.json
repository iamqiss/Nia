{
  "name": "time-push-notifications",
  "version": "1.0.0",
  "description": "Native push notifications for Time app with gRPC integration",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "build:ios": "cd ios && xcodebuild -workspace TimePushNotifications.xcworkspace -scheme TimePushNotifications -configuration Release -destination generic/platform=iOS build",
    "build:android": "cd android && ./gradlew assembleRelease",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "clean": "rm -rf node_modules && rm -rf ios/build && rm -rf android/build"
  },
  "keywords": [
    "react-native",
    "push-notifications",
    "grpc",
    "ios",
    "android",
    "fcm",
    "apns"
  ],
  "author": "Neo Qiss",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": ">=0.60.0"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.8",
    "react-native": "*"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-native": "^0.72.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/time-app/time-push-notifications.git"
  },
  "bugs": {
    "url": "https://github.com/time-app/time-push-notifications/issues"
  },
  "homepage": "https://github.com/time-app/time-push-notifications#readme"
}